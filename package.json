{
  "name": "shop_service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:profile": "node --inspect ./bin/www",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:e2e": "jest tests/e2e",
    "test:e2e:local": "bash scripts/run-e2e-tests.sh",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "if [ -x node_modules/.bin/husky ]; then node_modules/.bin/husky install; else echo 'husky not installed (prod install or missing devDeps) â€“ skipping'; fi",
    "pretest": "chmod +x tests/setup.sh && tests/setup.sh",
    "load:test": "autocannon -c 10 -d 30 http://localhost:3000",
    "load:search": "autocannon -c 10 -d 20 http://localhost:3000/shop/search-result?q=food",
    "load:products": "autocannon -c 5 -d 15 http://localhost:3000/product/dogfood"
  },
