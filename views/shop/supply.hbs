<div class="product">
    <h1>DOG SUPPLIES</h1>
    <div class="row">
        {{# each supplies}}
            {{# each this}}
                <div class="col-sm-6 col-md-4" style="padding: 25px">
                    <div class="img-thumbnail">
                        <img src="{{this.imagepath}}" alt="..." class="img-responsive">
                        <h3>{{this.Title}}</h3>
                        <p class="Description">Price:${{this.Price}}</p>
                        <div class="clearfix">
                            <a href="/cart/add/supply/{{this._id}}" class="btn btn-primary pull-right" role="button"><i class="fas fa-shopping-cart"></i> Add to cart</a>
                            &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;&nbsp;<a href="#" class="btn btn-secondary pull-right disabled" role="button">Buy Now</a>
                        </div>
                    </div>
                </div>
            {{/each}}
        {{/each}}
    </div>
</div>







